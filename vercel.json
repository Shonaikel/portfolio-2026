{
  "rewrites": [
    { "source": "/", "destination": "/src/index.html" },
    { "source": "/projects-pg", "destination": "/src/projects-pg.html" },
    { "source": "/myinformation", "destination": "/src/myinformation.html" }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
